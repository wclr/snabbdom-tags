{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "strict": true,    
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "outDir": "./build",
    "allowJs": false,
    "noEmit": false,
    "newLine": "LF",
    "declaration": true,
    "baseUrl": ".",
    "paths": {
      "snabbdom/*": ["node_modules/snabbdom/build/package/*"]
    }
  },
  
}
